#: modules/taxonomy.module:369
msgid "Parents"
msgstr "上位"

#: modules/taxonomy.module:369
msgid "Parent terms"
msgstr "上位の分類用語"

#: modules/taxonomy.module:377
msgid "Synonyms"
msgstr "同義語"

#: modules/taxonomy.module:377
